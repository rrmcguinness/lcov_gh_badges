{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": ["es2021"],
    "target": "es2021",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "../node_modules", "**/*.spec.ts"],
  "files": [
    "config.ts",
    "runner.ts",
    "stats.ts"
  ]
}